  "homepage": "./",
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "electron-pack": "npm run build && electron-builder -w --config electron-builder.json"
  },
  "devDependencies": {
    "electron": "^31.0.0",
    "electron-builder": "^23.6.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-scripts": "5.0.1"
  },
  "build": {
